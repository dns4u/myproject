 <head>
    
    <script src="js/jquery.min.js"></script>
    <script src="js/skel.min.js"></script>
    <script src="js/skel-layers.min.js"></script>
    <script src="js/init.js"></script>
    <script src="javascripts/javascript.js" type="text/javascript"></script>
      <link rel="icon" type="image/x-icon" href="E:\project front page\images\favicon.ico"/>
      <link rel="stylesheet" href="/css/skel.css" type="text/css" />
      <link rel="stylesheet" href="/css/style.css" type="text/css" />
      <link rel="stylesheet" href="/css/style-xlarge.css" type="text/css"/>
  </head> 
 <body class="landing">
  <div id ="main-container">
      <div id="navigation-bar">
       <header id="header"> 
        <h1><a></a></h1>
        
        <nav id="nav">
          <ul>
            <li><a id="home" href="/share"> <i class="fa fa-share-alt-square " style="color:#3cadd4;"></i>&nbsp; Share</li>
            <li><a id="search" href="/search"class="current"> <i class="fa fa-search" style="color:#FFFF99;"> </i> &nbsp;Search&nbsp;</a></li> 
            <li><a id="book" href="/myfile"> <i class="fa fa-folder-open-o" style="color:#add43c;"></i>&nbsp; My file</a></li>
            <li><a id="sign" href="/"> <i class="fa fa-sign-out " style="color:#e45230;"></i>&nbsp; Log out</a></li>
          </ul>
          
        </nav>
       </header> 
      </div> <!-- closing menu-bar -->
  <div class="share">
    <div class="status">
      <h1> <%=session['email']%></h1>
    </div>
          <form name="searchform" method="post" action="/search" onsubmit="return require()">
          <input type="text" name="search" id="file_search" value="<%=search_text%>" style="height:2em;width:30em;">
          <input type="submit" name="btmsearch" value="Search">
          </form>
       
     
           <div id="searchmenu">
     
          <ul>
         <a href="/all/<%=search_text%>"><li>All </li></a>
         <li> <a class="current" href="/video/<%=search_text%>">&nbsp;Videos&nbsp;</a></li>
          <a href="/image/<%=search_text%>"><li>Images</li></a>
          <a href="/pdf/<%=search_text%>"><li>PDFs</li></a>
          <a href="/audio/<%=search_text%>"><li>Audio</li></a>
           <a href="/text/<%=search_text%>"><li>Text</li></a>
          </ul>
        </div>
  <div id="search-item">
        <ul>
          <h1>Your search videos are:</h1>
          <%temp=0%>
          <%if(file_result != nil)%>
            <%file_result.each do|result| %>
            <%group_file.each do|group_file_result|%>
              <%if(result.file_file_name=~/#{Regexp.escape(search_text)}/)%>
              <% if(result.id==group_file_result.share_file_id)%>
                <%if(result.file_content_type=~/video/)%>
                  <%filename=result.file_file_name%>
                  <div id="eachresult">
                    <li><b><%=filename%></b><br>
                    <a href="/files/original/<%=filename%>"><%=result.file_file_name %></a></li>
                  </div>
                  <%temp=1%>
                <%end%>
              <%end%>
            <%end%>
          <%end%>
        <%end%>
        <%end%>
        </ul>
        <%if(temp==0)%>
        <p>Videos not found </p>
       <% end%>
      </div>
      </div>
      </div>
    </div>
 <footer id="footer">
        <div class="container">
          <section class="links">
            <div class="row">
              <section class="3u 6u(medium) 12u$(small)">
                
            </div>
          </section>
          <div class="row">
            <div class="8u 12u$(medium)">
              <ul class="copyright">
                <li>&copy; Share Knowledge. All rights reserved.</li>
                
              </ul>
            </div>
            <div class="4u$ 12u$(medium)">
              <ul class="icons">
                <li>
                  <a class="icon rounded fa-facebook"><span class="label">Facebook</span></a>
                </li>
                <li>
                  <a class="icon rounded fa-twitter"><span class="label">Twitter</span></a>
                </li>
                <li>
                  <a class="icon rounded fa-google-plus"><span class="label">Google+</span></a>
                </li>
                <li>
                  <a class="icon rounded fa-linkedin"><span class="label">LinkedIn</span></a>
                </li>
              </ul>
            </div>
          </div>
        </div>
    
  </div>
  
 </body>

</html> 