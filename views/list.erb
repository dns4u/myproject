  <body class="landing">
  <div id ="main-container">
      <div id="navigation-bar">
       <header id="header"> 
        <h1><a></a></h1>
        
        <nav id="nav">
           <ul>
            <li><a id="add" href="/register"> <i class="fa fa-plus" style="color:#e45230;"></i>&nbsp;Add Member&nbsp;</li>
            <!-- <li><a href="#">Generic</a></li> -->
            <li><a id="edit" href="/edit"> <i class="fa fa-edit" style="color:#309744;"></i>&nbsp; Edit</a></li>
            <li><a id="delete" href="/delete"> <i class="fa fa-trash" style="color:#a59755;"></i>&nbsp; Delete </a></li>
            <li><a id="logout" href="/"> <i class="fa fa-sign-out " style="color:#0091b3;"></i>&nbsp; Logout</a></li>
            <li><a id="list" class="current" href="/list"> <i class="fa fa-list-alt " style="color:#f091b3;"></i>&nbsp; List &nbsp;</a></li>
          </ul>
          
        </nav>
       </header> 
      </div> <!-- closing menu-bar -->
  
  <div class="share">
        <div class="status">
      <h1> <%=session['group']%></h1>
    </div>
  <div id="search-item">
        <ul>
      <center><h1>Information of member registered in this Group are</h1></center>
    
      <%check=0%>
      <table class="list">
      <tr><td>First name</td><td>Last Name</td><td>User Name</td><td>address</td><td>Mobile Number</td><td>Gender</td></tr>
    <% @group_list.each do |group|%>
      <% @employee_list.each do |employee| %>
         <%if(group.employee_id == employee.id)%>
         <tr><td><%=employee.first_name%></td><td><%=employee.last_name%></td><td><%=employee.email%></td><td><%=employee.address%></td><td><%=employee.mobile_number%></td><td><%=employee.gender%></td></tr>
         <%check=1%>
         <%end%>
      <%end%>
    <%end%>
  </table>
       <%if(check==0)%>
         <h1>No member register in this group </h1>
      <%end%>
     
   </ul>
        
      </div>
    </div>
   </div>
</div>

    <footer id="footer">
        <div class="container">
          <section class="links">
            <div class="row">
              <section class="3u 6u(medium) 12u$(small)">
                
            </div>
          </section>
          <div class="row">
            <div class="8u 12u$(medium)">
              <ul class="copyright">
                <li>&copy; Share Knowledge. All rights reserved.</li>
                
              </ul>
            </div>
