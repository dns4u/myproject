 <body class="landing">
  <div id ="main-container">
      <div id="navigation-bar">
       <header id="header"> 
        <h1><a></a></h1>
        
        <nav id="nav">
          <ul>
            <li><a id="home" href="/share"> <i class="fa fa-share-alt-square " style="color:#3cadd4;"></i>&nbsp; Share</li>
            <li><a id="search" href="/search" class="current"> <i class="fa fa-search" style="color:#FFFF99;"> </i> &nbsp;Search&nbsp;</a></li> 
            <li><a id="book" href="/myfile"> <i class="fa fa-folder-open-o" style="color:#add43c;"></i>&nbsp; My file</a></li>
            <li><a id="sign" href="/"> <i class="fa fa-sign-out " style="color:#e45230;"></i>&nbsp; Log out</a></li>
          </ul>
          
        </nav>
       </header> 
      </div> <!-- closing menu-bar -->
  <div class="share">
    <div class="status">
      <h1> <%=session['email']%></h1>
    </div>
          <form name="searchform" method="post" action="/search" onsubmit="return require()">
          <input type="text" name="search" id="file_search" value="<%=search_text%>" style="height:2em;width:30em;">
          <input type="submit" name="btmsearch" value="Search">
          </form>
       
     
           <div id="searchmenu">
     
          <ul>
         <a href="/all/<%=search_text%>"><li>All </li></a>
          <a href="/video/<%=search_text%>"><li>Videos</li></a>
          <li><a class="current" href="/image/<%=search_text%>">&nbsp;Images&nbsp;</a></li>
          <a href="/pdf/<%=search_text%>"><li>PDFs</li></a>
          <a href="/audio/<%=search_text%>"><li>Audio</li></a>
           <a href="/text/<%=search_text%>"><li>Text</li></a>
          </ul>
        </div>
  <div id="search-item">
        <ul>
        <h1>Your search images are:</h1>
        <%temp=0%>
        <%if(file_result != nil)%>
          <%file_result.each do|result| %>
          <%group_file.each do|group_file_result|%>
            <%if(result.file_file_name=~/#{Regexp.escape(search_text)}/)%>
             <% if(result.id==group_file_result.share_file_id)%>
              <%if(result.file_content_type=~/image/)%>
                <%filename=result.file_file_name%>
                <div class="eachresult">
                <li><b><%=filename%></b><br>
                <a href="/files/original/<%=filename%>"><%=result.file_file_name %></a></li>
                </div>
                <%temp=1%>
              <%end%>
            <%end%>
          <%end%>
        <%end%>
        <%end%>
        <%end%>
      </ul>
      <%if(temp==0)%>
      <p>Images not found </p>
      <% end%>
    </div>
  </div>
<footer id="footer">
        <div class="container">
          <section class="links">
            <div class="row">
              <section class="3u 6u(medium) 12u$(small)">
                
