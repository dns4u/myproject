      <center><h1>ShareKnowledge Group Creation Form</h1></center>
      <form method="post" action="" class="loginform" onsubmit="return groupRequired()">
        <table><tr><td>
        <label>Group name</label></td>
       <td> <input type="text" name="group_name" id="gname" onkeypress="return onlyAlphabets(event,this);" onblur="return groupLength(event,this);" pattern=".{3,}" required title="3 characters minimum" value="<%"#{group_member.group_name}"%>"></td></tr>
        <tr><td><label>Password</label></td>
        <td><input type="password" name="group_password" id="gpassword" onblur="return gpasswordLength(event,this);" pattern=".{6,}" required title="6 characters minimum" value="<%"#{group_member.group_password}"%>"></td></tr>
       <tr><td><label>Confirm Password</label></td>
        <td><input type="password"  id="gcpassword"></td></tr>
        <tr><td><label>Admin name</label></td>
       <td> <input type="text" name="admin_name" id="aname" value="<%"#{group_member.admin_name}"%>"></td></tr>
       <tr><td></td>
         <td> <input type="submit"  value="Create" id="create"></td></tr>
